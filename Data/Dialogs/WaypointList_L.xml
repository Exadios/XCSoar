<Form Caption="Select Waypoint">
  <Edit Name="name" Caption="Name" X="2" Width="61" Height="36" CaptionWidth="-1"/>

  <Edit Name="prpFltDistance" Caption="Distance" X="0" Width="65" Height="36" CaptionWidth="-1">
    <DataField DataType="enum"/>
  </Edit>

  <Edit Name="prpFltDirection" Caption="Direction" X="0" Width="65" Height="36" CaptionWidth="-1">
    <DataField DataType="enum"/>
  </Edit>

  <Edit Name="prpFltType" Caption="Type" X="0" Width="65" Height="36" CaptionWidth="-1">
    <DataField DataType="enum"/>
  </Edit>

  <Button Name="cmdSelect" Caption="Select" X="0" Y="-70" Width="65" Height="35" OnClick="OnSelectClicked" />
  <Button Name="cmdClose" Caption="Cancel" X="0" Y="-35" Width="65" Height="35" OnClick="OnCloseClicked" />

  <List Name="frmWaypointList" X="65" Y="0" Width="-1" Height="-1" />
</Form>
